import nltk
from nltk.tokenize import sent_tokenize, word_tokenize
from collections import Counter
from nltk.corpus import stopwords

def text_similarity(text1, text2):
    sentences1 = sent_tokenize(text1)
    sentences2 = sent_tokenize(text2)
    
    sentences1 = [sentence.lower() for sentence in sentences1]
    sentences2 = [sentence.lower() for sentence in sentences2]
    
    len1 = len(sentences1)
    len2 = len(sentences2)

    word_counts1 = [Counter(word_tokenize(sentence)) for sentence in sentences1]
    word_counts2 = [Counter(word_tokenize(sentence)) for sentence in sentences2]

    selected_words = ["и", "в", "не", "на", "с", "что", "а", "по", "к", "но", "как", "из", "у", "то", "за", "от", "о", "для", "же", "или", "уже", "до", "если", "при", "даже", "во", "со", "под", "без", "ли"]
    
    common_counts = []
    for word in selected_words:
        count1 = sum(word_counts1[i][word] for i in range(len(word_counts1)))
        count2 = sum(word_counts2[i][word] for i in range(len(word_counts2)))
        if count1 > 0 and count2 > 0:
            common_counts.append((count1, count2))
           # print(word, round(count1 / len1, 3), "", round(count2 / len2, 3))
            print(f"{word}\t{round(count1 / len1, 3)}\t{round(count2 / len2, 3)}")

    threshold = 0.05
    matches1 = sum(abs(count1 / len1 - count2 / len2) < threshold for count1, count2 in common_counts)
    matches2 = sum(abs(count2 / len2 - count1 / len1) < threshold for count1, count2 in common_counts)

    if matches1 > 0.7 * len(common_counts) or matches2 > 0.7 * len(common_counts):
        print("")
        print("  У введённых текстов вероятно один автор, так как по списку частотных")
        print(" служебных слов, более 70% частотных слов, которые встречались в обоих текстах,")
        print(" имеют разброс менее чем на 5 сотых частотности (максимальное значение 1)")
        print("")
    else:
        print("")
        print("  У введённых текстов вероятно НЕ один автор, так как по списку частотных")
        print(" служебных слов, МЕНЕЕ 70% частотных слов, которые встречались в обоих текстах,")
        print(" имеют разброс менее чем на 5 сотых частотности (максимальное значение 1)")
        print("")
  
